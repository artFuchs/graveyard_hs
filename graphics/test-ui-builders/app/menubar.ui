<?xml version="1.0" standalone="no"?>
<interface>
  <object class="GtkMenuBar" id="menubar1">
    <property name="visible">1</property>
    <child>
      <object class="GtkMenuItem" id="file_item">
        <property name="visible">1</property>
        <property name="label">_File</property>
        <property name="use-underline">1</property>
        <child type="submenu">
          <object class="GtkMenu" id ="file_menu">
            <child>
              <object class="GtkMenuItem" id="new_item">
                <property name="visible">1</property>
                <property name="label">_New Project</property>
                <property name="use-underline">1</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="open_item">
                <property name="visible">1</property>
                <property name="label">_Open Project </property>
                <property name="use-underline">1</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="save_item">
                <property name="visible">1</property>
                <property name="label">_Save Project</property>
                <property name="use-underline">1</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="save_as_item">
                <property name="visible">1</property>
                <property name="label">Save Project _As</property>
                <property name="use-underline">1</property>
              </object>
            </child>
            <child>
              <object class="GtkSeparatorMenuItem" id="file-sep">
                <property name="visible">1</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="save_graph_item">
                <property name="visible">1</property>
                <property name="label">Save _Graph</property>
                <property name="use-underline">1</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="open_graph_item">
                <property name="visible">1</property>
                <property name="label">Open G_raph</property>
                <property name="use-underline">1</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkMenuItem" id="edit_item">
        <property name="visible">1</property>
        <property name="label">_Edit</property>
        <property name="use-underline">1</property>
        <child type="submenu">
          <object class="GtkMenu" id="edit_menu">

          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkMenuItem" id="view_item">
        <property name="visible">1</property>
        <property name="label">_View</property>
        <property name="use-underline">1</property>
        <child type="submenu">
          <object class="GtkMenu" id="view_menu">

          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkMenuItem" id="help_item">
        <property name="visible">1</property>
        <property name="label">_Help</property>
        <property name="use-underline">1</property>
        <child type="submenu">
          <object class="GtkMenu" id="help_menu">

          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
